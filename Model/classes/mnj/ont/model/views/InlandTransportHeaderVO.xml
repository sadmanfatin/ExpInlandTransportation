  <SQLQuery>
    <![CDATA[SELECT InlandTransportHeaderEO.INLAND_TRANS_HEADER_ID, 
       InlandTransportHeaderEO.TRANSPORTATION_AGENCY_NAME, 
       InlandTransportHeaderEO.TRANSPORTATION_COMPANY_NAME, 
       InlandTransportHeaderEO.VEHICLE_NUMBER, 
       InlandTransportHeaderEO.VEHICLE_ARRIVAL_DATE, 
       InlandTransportHeaderEO.VEHICLE_DEPARTURE_DATE, 
       InlandTransportHeaderEO.EX_FACTORY_DATE, 
       trunc(InlandTransportHeaderEO.EX_FACTORY_DATE)EX_FACTORY_DATE_ONLY , 
       InlandTransportHeaderEO.DRIVER_NAME, 
       InlandTransportHeaderEO.DRIVER_MOBILE_NUMBER, 
       InlandTransportHeaderEO.DRIVER_LICENSE_NUMBER, 
       InlandTransportHeaderEO.ESCORT_NAME, 
       InlandTransportHeaderEO.ESCORT_MOBILE_NUMBER, 
       InlandTransportHeaderEO.STATUS_MODE, 
       InlandTransportHeaderEO.ORG_ID, 
       InlandTransportHeaderEO.LOCK_NUMBER, 
       InlandTransportHeaderEO.REMARKS,
        InlandTransportHeaderEO.DEPARTURE_TIME,
        InlandTransportHeaderEO.EMPTY_CARTON,
        InlandTransportHeaderEO.GUM_TAP,
       InlandTransportHeaderEO.LAST_UPDATE_LOGIN, 
       InlandTransportHeaderEO.LAST_UPDATE_DATE, 
       InlandTransportHeaderEO.LAST_UPDATED_BY, 
       InlandTransportHeaderEO.CREATION_DATE, 
       InlandTransportHeaderEO.CREATED_BY, 
       InlandTransportHeaderEO.ATTRIBUTE1, 
       InlandTransportHeaderEO.ATTRIBUTE2, 
       InlandTransportHeaderEO.ATTRIBUTE3, 
       InlandTransportHeaderEO.ATTRIBUTE4, 
       InlandTransportHeaderEO.ATTRIBUTE5, 
       InlandTransportHeaderEO.ATTRIBUTE6, 
       InlandTransportHeaderEO.ATTRIBUTE7, 
       InlandTransportHeaderEO.ATTRIBUTE8, 
       InlandTransportHeaderEO.ATTRIBUTE9, 
       InlandTransportHeaderEO.ATTRIBUTE10, 
       InlandTransportHeaderEO.ATTRIBUTE11, 
       InlandTransportHeaderEO.ATTRIBUTE12, 
       InlandTransportHeaderEO.ATTRIBUTE13, 
       InlandTransportHeaderEO.ATTRIBUTE14, 
       InlandTransportHeaderEO.ATTRIBUTE15, 
       InlandTransportHeaderEO.ATTRIBUTE16, 
       InlandTransportHeaderEO.ATTRIBUTE17, 
       InlandTransportHeaderEO.ATTRIBUTE18, 
       InlandTransportHeaderEO.ATTRIBUTE19, 
       InlandTransportHeaderEO.ATTRIBUTE20, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM1, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM2, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM3, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM4, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM5, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM6, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM7, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM8, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM9, 
       InlandTransportHeaderEO.ATTRIBUTE_NUM10, 
       InlandTransportHeaderEO.ATTRIBUTE_DATE1, 
       InlandTransportHeaderEO.ATTRIBUTE_DATE2, 
       InlandTransportHeaderEO.ATTRIBUTE_DATE3, 
       InlandTransportHeaderEO.ATTRIBUTE_DATE4, 
       InlandTransportHeaderEO.ATTRIBUTE_DATE5,
      (SELECT INTERNAL_ADDRESS_LINE 
       FROM HR_ORGANIZATION_UNITS   
       WHERE ORGANIZATION_ID = InlandTransportHeaderEO.ORG_ID )ORG_NAME,
       InlandTransportHeaderEO.RCV_FLAG,
       InlandTransportHeaderEO.ESCORT_ID ,
       line.STYLE_NO , 
        line.ORDER_BPO ,  
            line.CFS_NAME  ,
            line.PACK_LIST   ,
           line.INVOICE_NO  
FROM IEDOC_EXP_INLAND_TRAN_HEADERS InlandTransportHeaderEO  ,

(SELECT L.INLAND_TRANS_HEADER_ID ,
       (SELECT LISTAGG (STYLE_NO, ', ') WITHIN GROUP (ORDER BY STYLE_NO)
          FROM (SELECT DISTINCT (LL.STYLE_NO)
                  FROM IEDOC_EXP_INLAND_TRAN_LINES LL
                 WHERE LL.INLAND_TRANS_HEADER_ID = L.INLAND_TRANS_HEADER_ID
                 )
               ) STYLE_NO , 
               
          (SELECT LISTAGG (ORDER_BPO, ', ') WITHIN GROUP (ORDER BY ORDER_BPO)
          FROM (SELECT DISTINCT (LL.ORDER_BPO)
                  FROM IEDOC_EXP_INLAND_TRAN_LINES LL
                   WHERE LL.INLAND_TRANS_HEADER_ID = L.INLAND_TRANS_HEADER_ID
                 )
               ) ORDER_BPO,       
               
               
               (SELECT LISTAGG (CFS_NAME, ', ') WITHIN GROUP (ORDER BY CFS_NAME)
          FROM (SELECT DISTINCT (LL.CFS_NAME)
                  FROM IEDOC_EXP_INLAND_TRAN_LINES LL
                   WHERE LL.INLAND_TRANS_HEADER_ID = L.INLAND_TRANS_HEADER_ID
                 )
               ) CFS_NAME   ,

      (SELECT LISTAGG (PACK_LIST, ', ') WITHIN GROUP (ORDER BY PACK_LIST)
          FROM (SELECT DISTINCT (LL.PACK_LIST)
                  FROM IEDOC_EXP_INLAND_TRAN_LINES LL
                   WHERE LL.INLAND_TRANS_HEADER_ID = L.INLAND_TRANS_HEADER_ID
                 )
               ) PACK_LIST ,
        (SELECT LISTAGG (INVOICE_NO, ', ') WITHIN GROUP (ORDER BY INVOICE_NO)
          FROM (SELECT DISTINCT (LL.INVOICE_NO)
                  FROM IEDOC_EXP_INLAND_TRAN_LINES LL
                   WHERE LL.INLAND_TRANS_HEADER_ID = L.INLAND_TRANS_HEADER_ID
                 )
               ) INVOICE_NO 
                
 
     
FROM IEDOC_EXP_INLAND_TRAN_LINES L 
group by L.INLAND_TRANS_HEADER_ID

) line
 where InlandTransportHeaderEO.INLAND_TRANS_HEADER_ID = line.INLAND_TRANS_HEADER_ID(+)]]>
  </SQLQuery>
